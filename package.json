{
  "name": "learn-angular-multiapps",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:app1": "ng serve --project application1",
    "start:app2": "ng serve --project application2",
    "start:red": "ng serve --project beehive-red",
    "start:green": "ng serve --project beehive-green",
    "start:blue": "ng serve --project beehive-blue",
    "start:demo": "ng serve --project demo-form-security",
    "start:material": "ng serve --project demo-material",
    "start:unittest": "ng serve --project demo-unit-test",
    "build": "ng build --configuration production",
    "build:red": "ng build --project beehive-red --configuration production",
    "build:green": "ng build --project beehive-green --configuration production",
    "build:blue": "ng build --project beehive-blue --configuration production",
    "build:demo": "ng build --project demo-form-security --configuration production",
    "build:material": "ng build --project demo-material --configuration production",
    "build:unittest": "ng build --project demo-unit-test --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:unit": "ng test --project demo-unit-test",
    "dev:ssr": "ng run learn-angular-multiapps:serve-ssr",
    "serve:ssr": "node dist/learn-angular-multiapps/server/main.js",
    "build:ssr": "ng build && ng run learn-angular-multiapps:server",
    "prerender": "ng run learn-angular-multiapps:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/platform-server": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@nguniversal/express-engine": "^12.1.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.15.2",
    "express-session": "^1.17.3",
    "ngx-cookie-service": "^12.0.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.0",
    "@nguniversal/builders": "^12.1.3",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.17.10",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.20.55",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.1.1",
    "typescript": "~4.3.5"
  }
